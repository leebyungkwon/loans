<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>

   <!-- main -->
   <definition name="main-fo-layout" template="/WEB-INF/layout/default-fo-layout.jsp">
       <put-attribute name="js-fo"		value="/WEB-INF/layout/default/js-fo.jsp" />
       <put-attribute name="css-fo"	value="/WEB-INF/layout/default/css-fo.jsp" />
       <put-attribute name="header-fo" value="/WEB-INF/layout/default/header-fo.jsp" />
       <put-attribute name="body-fo"	value="" />
       <put-attribute name="footer-fo" value="/WEB-INF/layout/default/footer-fo.jsp" />
   </definition>
   
   <definition name="fo/*" extends="main-fo-layout">
       <put-attribute name="body-fo" value="/WEB-INF/views/fo/{1}.jsp" />
   </definition>
   <definition name="fo/*/*" extends="main-fo-layout">
       <put-attribute name="body-fo" value="/WEB-INF/views/fo/{1}/{2}.jsp" />
   </definition>
   
</tiles-definitions>