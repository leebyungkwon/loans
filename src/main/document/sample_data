ALTER TABLE tb_lc_mas01 ADD COLUMN reg_addr varchar(200); 							-- 등록기준지 주소
ALTER TABLE tb_lc_mas01 ADD COLUMN reg_addr_detail varchar(200); 					-- 등록기준지 상세 주소
ALTER TABLE tb_lc_mas01 ADD COLUMN other_field varchar(100); 						-- 영위하는 다른 업종
ALTER TABLE tb_lc_mas01 ADD COLUMN within_govr varchar(100); 						-- 관할 검찰청 또는 지청
ALTER TABLE tb_lc_mas01 ADD COLUMN addr_base varchar(200); 							-- 주소
ALTER TABLE tb_lc_mas01 ADD COLUMN pl_stat_req_seq INTEGER; 						-- 처리상태 요청자시퀀스
ALTER TABLE tb_lc_mas01 ADD COLUMN pl_stat_req_path char(1); 						-- 처리상태 요청경로

ALTER TABLE tb_lc_mas01_hist ADD COLUMN reg_seq INTEGER;
ALTER TABLE tb_lc_mas01_hist ADD COLUMN reg_path char(1) not null default 'B';
ALTER TABLE tb_lc_mas01_hist ADD COLUMN reg_addr varchar(200);
ALTER TABLE tb_lc_mas01_hist ADD COLUMN reg_addr_detail varchar(200);
ALTER TABLE tb_lc_mas01_hist ADD COLUMN other_field varchar(100);
ALTER TABLE tb_lc_mas01_hist ADD COLUMN within_govr varchar(100);
ALTER TABLE tb_lc_mas01_hist ADD COLUMN pl_stat_req_seq varchar(200);
ALTER TABLE tb_lc_mas01_hist ADD COLUMN addr_base varchar(200);
ALTER TABLE tb_lc_mas01_hist ADD COLUMN pl_stat_req_seq INTEGER; 
ALTER TABLE tb_lc_mas01_hist ADD COLUMN pl_stat_req_path char(1); 

ALTER TABLE tb_lc_mas01_imwon ADD COLUMN position_cd char(1); 						-- 직위
ALTER TABLE tb_lc_mas01_imwon ADD COLUMN proper_cd char(1); 						-- 임원자격 적합여부

ALTER TABLE tb_lc_mas01_step ADD COLUMN pl_stat_req_seq INTEGER;					-- 처리상태 요청자시퀀스
ALTER TABLE tb_lc_mas01_step ADD COLUMN pl_stat_req_path char(1);					-- 처리상태 요청경로

ALTER TABLE tb_lc_notice ADD COLUMN notice_disp_cd char(1) not null default '1'; 	-- 공지대상(1 : 금융회사 / 2 : 모집인)

-- 코드(property 값 수정)
CAR001 : 1,2만 모두 property01,property02 값 Y로 수정
PRD001 : 01,05만 property01 값 Y로 수정	
EXP001 : 1,2만 property01 값 Y로 수정

UPDATE tb_lc_cod01_detail SET property01 = 'Y', property02 = 'Y' WHERE code_mst_cd = 'CAR001' AND code_dtl_cd IN ('1','2');
UPDATE tb_lc_cod01_detail SET property01 = 'Y' WHERE code_mst_cd = 'PRD001' AND code_dtl_cd IN ('01','05');
UPDATE tb_lc_cod01_detail SET property01 = 'Y' WHERE code_mst_cd = 'EXP001' AND code_dtl_cd IN ('1','2');

