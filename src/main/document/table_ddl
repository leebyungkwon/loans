	, pl_business_no varchar(10)
	, comp_phone_no varchar(20)
	, corp_found_date varchar(8)
	, capital varchar(20)
	, voting_stock_cnt varchar(20)
	, pl_pay_stat varchar(2)
	, com_reg_date varchar(8)
	
